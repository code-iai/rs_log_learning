  <analysisEngineMetaData>
    <name>MindPalaceTrain</name>
    <description>MindPalace pipeline for training</description>
    <version>1.0</version>
    <vendor/>
    <configurationParameters searchStrategy="none">
      <configurationParameter>
        <name>camera_config_files</name>
        <description>path to config file that holds info about camera topics and tf frame names</description>
        <type>String</type>
        <multiValued>true</multiValued>
        <mandatory>true</mandatory>
        <overrides>
          <parameter>CollectionReader/camera_config_files</parameter>
        </overrides>
      </configurationParameter>
      <configurationParameter>
        <name>clearStorageOnStart</name>
        <description/>
        <type>Boolean</type>
        <multiValued>false</multiValued>
        <mandatory>false</mandatory>
        <overrides>
          <parameter>StorageWriter/clearStorageOnStart</parameter>
        </overrides>
      </configurationParameter>
      <configurationParameter>
        <name>db</name>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>false</mandatory>
 	<overrides>
          <parameter>StorageWriter/db</parameter>
 	</overrides>
      </configurationParameter>

    </configurationParameters>
    <configurationParameterSettings>
      <nameValuePair>
        <name>camera_config_files</name>
        <value>
          <array>
            <string>config_mongodb_playback_annotated.ini</string>
          </array>
        </value>
      </nameValuePair>
      <nameValuePair>
        <name>clearStorageOnStart</name>
        <value>
          <boolean>false</boolean>
        </value>
      </nameValuePair>
      <nameValuePair>
        <name>db</name>
        <value>
          <string>Scenes_learned</string>
        </value>
      </nameValuePair>
    </configurationParameterSettings>
    <flowConstraints>
      <fixedFlow>
     <!-- <node>Trigger</node> -->
      <node>CollectionReader</node>
      <node>ImagePreprocessor</node>
      <node>MindPalaceAnnotator</node>
      <node>StorageWriter</node>
      </fixedFlow>
    </flowConstraints>
    <typePriorities/>
    <fsIndexCollection>
      <fsIndexes>
        <fsIndexDescription>
          <label>ClusterIndex</label>
          <typeName>iai_rs.pcl.PointIndices</typeName>
          <kind>bag</kind>
        </fsIndexDescription>
        <fsIndexDescription>
          <label>CloudIndex</label>
          <typeName>iai_rs.pcl.PointCloud</typeName>
          <kind>bag</kind>
        </fsIndexDescription>
        <fsIndexDescription>
          <label>general</label>
          <typeName>uima.cas.TOP</typeName>
          <kind>bag</kind>
        </fsIndexDescription>
      </fsIndexes>
    </fsIndexCollection>
    <capabilities>
      <capability>
        <inputs/>
        <outputs/>
        <languagesSupported>
          <language>x-unspecified</language>
        </languagesSupported>
      </capability>
    </capabilities>
    <operationalProperties>
      <modifiesCas>true</modifiesCas>
      <multipleDeploymentAllowed>true</multipleDeploymentAllowed>
      <outputsNewCASes>false</outputsNewCASes>
    </operationalProperties>
  </analysisEngineMetaData>
</taeDescription>
